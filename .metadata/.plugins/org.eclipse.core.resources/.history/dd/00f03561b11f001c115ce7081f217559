package assignment1;

public class Actstring1 {
	public static void main(String[] args) 
	{
		String lang="The quick brown fox jumps over the lazy dog";
		//System.out.println("Indexof()::"+lang.indexOf(13));
		System.out.println("Indexof()::"+lang.charAt(12));
		System.out.println("Indexof()::"+lang.lastIndexOf("is"));
		System.out.println(lang + "and killed it");
		System.out.println("EndsWith:: "+lang.endsWith("dogs"));
		System.out.println(lang=="The quick brown Fox jumps over the lazy Dog");
		System.out.println(lang=="THE QUICK BROWN FOX JUMPS OVER THE LAZY DOG");
		System.out.println("Indexof()::"+lang.indexOf("a"));
		System.out.println("LastIndexof:: "+lang.lastIndexOf("e"));
		System.out.println("Length:: "+lang.length());
		System.out.println(lang.matches("The quick brown Fox jumps over the lazy Dog"));
		System.out.println(lang.replaceAll("The","A"));
		String[] arrOfStr=lang.split("fox");
		for(int i=0; i< arrOfStr.length; i++)
        {
            System.out.println("str["+i+"] : "+arrOfStr[i]);
        }
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	

}
